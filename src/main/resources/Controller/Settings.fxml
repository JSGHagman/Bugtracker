<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: lightBlue;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Controller">
   <children>
      <Pane prefHeight="398.0" prefWidth="81.0" style="-fx-background-color: #002366;">
