<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="BOTTOM_RIGHT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="View.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Test!" onAction="#onHelloButtonClick"/>
    <Label fx:id="goodbyeText"/>
    <Button text="GoodBye!" onAction="#goodbyeButtonClick"/>
</VBox>
